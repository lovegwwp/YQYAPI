<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jyss.yqy.mapper.OrderMoneyMapper" >

    <resultMap type="com.jyss.yqy.entity.OrderMoney" id="BaseResultMap">
        <id property="id" column="id"  />
        <result property="uId" column="u_id"  />
        <result property="orderSn" column="order_sn"  />
        <result property="type" column="type"  />
        <result property="score" column="score"  />
        <result property="jyScore" column="jy_score"  />
        <result property="payType" column="pay_type"  />
        <result property="detail" column="detail"  />
        <result property="status" column="status"  />
        <result property="createTime" column="create_time"  />
    </resultMap>


    <insert id="insertOrderMoney" parameterType="com.jyss.yqy.entity.OrderMoney" useGeneratedKeys="true" keyProperty="id">
        insert into o_order_money (id,u_id,order_sn,type,score,jy_score,pay_type,detail,status,create_time)
        values (null,#{uId},#{orderSn},#{type},#{score},#{jyScore},#{payType},#{detail},#{status},#{createTime})
    </insert>




</mapper>